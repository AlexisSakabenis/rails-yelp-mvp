<%= form_with model: @restaurant, local: true do |form| %>
  <%= form.label :name %><br/>
  <%= form.text_field :name %><br/>

  <%= form.label :address %><br/>
  <%= form.text_field :address %><br/>

  <%= form.label :phone_number %><br/>
  <%= form.text_field :phone_number %><br/>

  <%= form.label :category %><br/>
  <%= form.text_field :category %><br/>

  <%= form.submit %>
<% end %>
